 	<resultMap type="com.saitejn.pojo.Jb_feedback02" id="uservarMap">
 		
 		<result column="passivevar" property="passivevar"/>
 		<collection property="feedback03s" ofType="com.saitejn.pojo.Jb_feedback03">
 		<id column="id" property="id"/>
 			<result column="activevar" property="activevar"/>
 			
 		</collection>
 	</resultMap>
 	<select id="selectFeedBack01" resultType="int">
 		select top 1 id from jb_feedback01  order by id desc where type="1"
 	</select>
 	
 	<select id="selectvarByid_jb" parameterType="int" resultMap="uservarMap">
 		select jb_feedback03.id ,jb_feedback02.passivevar,jb_feedback03.activevar from  jb_feedback02,jb_feedback03 where jb_feedback03.parid = jb_feedback02.id and jb_feedback02.timid =#{0}
 	</select>
 	
