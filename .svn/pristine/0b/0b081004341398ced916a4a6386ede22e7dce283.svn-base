package com.saitejn.test;

import java.util.Date;

public class User {
	 
	  private int number;
	  private String name;
	  private int age;
	  private boolean sex;
	  private Date birthday;
	public int getNumber() {
		return number;
	}
	public void setNumber(int number) {
		this.number = number;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public boolean isSex() {
		return sex;
	}
	public void setSex(boolean sex) {
		this.sex = sex;
	}
	public Date getBirthday() {
		return birthday;
	}
	public void setBirthday(Date birthday) {
		this.birthday = birthday;
	}
	public User(int number, String name, int age, boolean sex, Date birthday) {
		super();
		this.number = number;
		this.name = name;
		this.age = age;
		this.sex = sex;
		this.birthday = birthday;
	}
	public User(int number, String name, int age, boolean sex) {
		super();
		this.number = number;
		this.name = name;
		this.age = age;
		this.sex = sex;
	}
	public User(int number, String name) {
		super();
		this.number = number;
		this.name = name;
	}
	
	  
	  
	  
}
