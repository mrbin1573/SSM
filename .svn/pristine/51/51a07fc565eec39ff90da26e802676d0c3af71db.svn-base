        /*设置iframe高度*/
        function ifrmHt(){
            /*获取浏览器文本框高度*/
            var windowHeight = $(window).height();
            var ht = windowHeight-70 + "px";
            /*设置iframe高度*/
            $("#iframepage").css("height", ht);
        }

        /* 三级菜单弹出与隐藏 */
